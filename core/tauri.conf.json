{
  "productName": "DACM",
  "version": "0.1.0",
  "identifier": "com.dacm.desktop",
  "build": {
    "devUrl": "http://localhost:1420",
    "frontendDist": "../view/dist",
    "beforeDevCommand": {
      "script": "npm run dev",
      "cwd": "../view"
    },
    "beforeBuildCommand": {
      "script": "npm run build",
      "cwd": "../view"
    }
  },
  "app": {
    "windows": [
      {
        "title": "DACM",
        "width": 1280,
        "height": 860
      }
    ]
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/icon-32.png",
      "icons/icon-128.png",
      "icons/icon-256.png",
      "icons/icon-512.png",
      "icons/icon.png",
      "icons/icon.icns"
    ]
  }
}
